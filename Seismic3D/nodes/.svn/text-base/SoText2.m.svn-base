#import "SoText2.h"

@interface SoText2()

@property (strong, nonatomic) NSString* string;
@property (assign) float spacing;
@property (assign) Justification justification;

@end

@implementation SoText2
@synthesize string = _string;
@synthesize spacing = _spacing;
@synthesize justification = _justification;

- (id) init
{
    self = [super init];
    if (self) {
        self.string = @"";
        self.spacing = 1;
        self.justification = Left;
    }
    
    return self;
}

@end
