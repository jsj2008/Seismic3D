#import "SoSphere.h"

@interface SoSphere()

@property (assign) float radius;

@end

@implementation SoSphere
@synthesize radius = _radius;

- (id) init
{
    self = [super init];
    if (self) {
        self.radius = 1;
    }
    
    return self;
}

@end
